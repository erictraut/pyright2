{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "outDir": "./out",
        "paths": {
            "typeserver/*": [
                "../typeserver/src/*"
            ],
        },
    },
    "include": [
        "../typeserver/src/**/*",
        "src/**/*",
        "**/*.js"
    ],
    "exclude": [
        "../typeserver/src/tests/**/*",
        "node_modules",
        "dist",
        "out"
    ]
}
